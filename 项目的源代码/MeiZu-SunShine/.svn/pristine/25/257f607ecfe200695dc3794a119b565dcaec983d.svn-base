.main{
	width: 100%;
	padding: 0;
	margin-bottom: 80px;
}
.banner{
	height: 480px;
	width: 100%;
	/*border: 1px solid red;*/
	/*position:absolute;*/
	/*display: block;*/
	/*margin: 0 auto;*/
	top:120px;
	left:0;
	z-index: -1;
	padding: 0;
	margin: 0;
}
.banner img{
	width: 100%;
	height: 100%;
}
.banner ul{
	position: absolute;
	top: 450px;
	left: 730px;
}
.banner li{
	display: block;
	width: 10px;
	height: 10px;
	border: 1px solid #BBB;
	border-radius: 5px;
	background-color: #FFF;
	z-index: 22222222;
	float: left;
	margin-right: 10px;
}
.banner_2{
	height: 480px;
	/*border: 1px solid red;*/
	padding: 0;
	top:120px;
	position: absolute;
	background: rgba(205,205,205,0.8);
}
.banner_2 a{
	display: block;
	height: 50px;
	line-height: 50px;
	padding-left: 40px;
	font-size: 14px;
	color: #3a3a3a;
	margin-top: 9px;
}
.banner_2 a:hover{
	background: #FFF;
}
.banner_3{
	/*border: 1px solid red;*/
	position: relative;
	padding: 0;
	margin: 0;
	height: 100%;
}
.banner_4{
	/*border: 1px solid green;*/
	background: #FFF;
	height: 480px;
	width: 470px;
	padding: 0;
	position: absolute;
	top: 120px;
	display: none;
	padding-left:1px;
}
.banner_6{
	width: 50px;
	height: 50px;
	/*border: 1px solid red;*/
	vertical-align:middle;
	margin-left: 10px;
	float: left;
	margin-top: 38px;
	text-align: center;
	line-height: 50px;
	clear: both;
}
.banner_7{
	height: 50px;
	/*border: 1px solid red;*/
	vertical-align:middle;
	margin-left: 10px;
	float: left;
	margin-top: 38px;
	text-align: center;
	line-height: 50px;
}

/*banner结束*/

